{
    "name": "dnwjn/nova-button",
    "description": "Nova button package",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/dnwjn/nova-button",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Dillingham",
            "email": "bdillingham88@gmail.com"
        },
        {
            "name": "dnwjn",
            "email": "hello@dnwjn.dev"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "illuminate/support": "^8.81.0",
        "illuminate/events": "^v8.81.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Dnwjn\\NovaButton\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dnwjn\\NovaButton\\NovaButtonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
